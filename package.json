{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "post": "hexo new post",
    "draft": "hexo new draft",
    "publish": "hexo publish",
    "dev": "pm2 start ecosystem.config.js && pm2 logs",
    "dev:stop": "pm2 delete ecosystem.config.js",
    "git:push": "git add . && git commit -m '更新博客' && git push",
    "deploy": "rm -rf public && hexo deploy",
    "build": "hexo generate",
    "clean": "hexo clean",
    "server": "hexo server"
  },
  "hexo": {
    "version": "6.2.0"
  },
  "dependencies": {
    "hexo": "^6.2.0",
    "hexo-deployer-git": "^3.0.0",
    "hexo-generator-archive": "^1.0.0",
    "hexo-generator-category": "^1.0.0",
    "hexo-generator-index": "^2.0.0",
    "hexo-generator-searchdb": "^1.4.0",
    "hexo-generator-tag": "^1.0.0",
    "hexo-renderer-ejs": "^2.0.0",
    "hexo-renderer-marked": "^5.0.0",
    "hexo-renderer-stylus": "^2.1.0",
    "hexo-server": "^3.0.0",
    "hexo-theme-landscape": "^0.0.3",
    "hexo-theme-next": "^8.12.2",
    "pm2": "^5.2.0"
  }
}